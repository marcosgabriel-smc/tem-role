<div class="container event-list-print" data-controller="print">
  <h1>
    <%= "Evento: #{@event_list.event.name}" %>
    <br>
    <%= @event_list.name %>
  </h1>
  <ul>
    <% @event_list.users.each do |user| %>
      <li>
        <a href="<%= user_path(user) %>">
          <% default_avatar = asset_path('member_icon.png') %>
          <%= image_tag(
          "https://kitt.lewagon.com/placeholder/users/#{user.username}",
          alt: "avatar",
          class: "avatar my-1",
          onerror: "this.onerror=null;this.src='#{default_avatar}'"
        ) %>
          <%= user.username %>
        </a>
      </li>
    <% end %>
  </ul>
</div>
